{
  "printWidth": 200,
  "eslintIntegration": false,
  "stylelintIntegration": true,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "files.insertFinalNewline": true,
  "useTabs": false,
  "endOfLine": "auto",
  "ignorePath": ".gnore"
}